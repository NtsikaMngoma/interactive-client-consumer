{
    "name": "interactive-client/laravel-api-consumer",
    "description": "Laravel API Consumer as a frontend service",
    "keywords": [
        "interactive-client",
        "laravel api consumer",
        "laravel",
        "api consumer",
        "rest-api"
    ],
    "homepage": "https://github.com/NtsikaMngoma/laravel-api-consumer",
    "license": "MIT",
    "authors": [
        {
            "name": "Ntsika Mark Mngoma",
            "email": "mark.mngoma@interactivesa.co.za",
            "homepage": "https://interactivesa.coza",
            "role": "PHP/Java Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "kitetail/zttp": "^0.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0.*"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "InteractiveClient\\ApiConsumer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InteractiveClient\\ApiConsumer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "InteractiveClient\\ApiConsumer\\ApiConsumerServiceProvider"
            ]
        }
    }
}
